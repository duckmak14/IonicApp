@model List<WebModels.Partner>

@{
    Layout = "~/Areas/Admin/Views/Shared/_LayoutEmpty.cshtml";
}

@using (Html.BeginForm("ExportExcel", "ManagePartner", FormMethod.Post, new { @class = "form-horizontal fix" }))
{
    @*<p>Bạn có muốn xuất file Excel?</p>*@
    <div class="alert alert-info">
        @Html.Raw("Bạn có muốn xuất file Excel?") <br />
        @foreach (var item in Model)
        {
            @Html.Hidden("[" + Model.IndexOf(item) + "].PartnerName", @item.PartnerName)
            @Html.Hidden("[" + Model.IndexOf(item) + "].Address", @item.Address)
            @Html.Hidden("[" + Model.IndexOf(item) + "].Email", @item.Email)
            @Html.Hidden("[" + Model.IndexOf(item) + "].ID", @item.ID)
            @Html.Hidden("[" + Model.IndexOf(item) + "].Email", @item.Email)
            @Html.Hidden("[" + Model.IndexOf(item) + "].Mobile", @item.Mobile)
        }
    </div>
    <div class="form-actions">
        <button class="btn btn-primary" type="submit">Xác nhận</button>
    </div>
    @Html.AntiForgeryToken()
}
<script type="text/javascript">

    function deletes_success() {
        top.$("#window").data("kendoWindow").close();
        top.$("#grid").data("kendoGrid").dataSource.read();
    }

    $(document).ready(function () {
        top.winsetup("Xuất file Quản lý đối tác", 400, 85, true);
    });
</script>                                                                                        